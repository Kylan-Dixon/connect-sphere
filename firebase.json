
{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "headers": [
      {
        "source": "/_next/static/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/**/!(*.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "private, no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}
